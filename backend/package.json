{
	"name": "backend",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"build": "tsc",
		"start": "npm run build && node ./bin/www",
		"devstart": "npm run build && nodemon --legacy-watch ./bin/www",
		"serverstart": "DEBUG=express-locallibrary-tutorial:* npm run devstart",
		"test": "jest",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0",
		"lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
		"format": "prettier --check .",
		"format:fix": "prettier --write ."
	},
	"dependencies": {
		"@types/express": "^4.17.21",
		"cookie-parser": "~1.4.4",
		"cors": "^2.8.5",
		"debug": "~2.6.9",
		"dotenv": "^16.4.1",
		"express": "^4.18.2",
		"http-errors": "~1.6.3",
		"morgan": "~1.9.1",
		"pg": "^8.11.3",
		"pg-hstore": "^2.3.4",
		"pug": "^3.0.2",
		"reflect-metadata": "^0.2.1",
		"sequelize": "^6.36.0",
		"supertest": "^6.3.4",
		"typeorm": "^0.3.20",
		"winston": "^3.11.0",
		"winston-daily-rotate-file": "^4.7.1"
	},
	"devDependencies": {
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"jest": "^29.7.0",
		"nodemon": "^3.0.3",
		"prettier": "^3.2.5",
		"typescript": "^5.3.3"
	},
	"jest": {
		"testMatch": [
			"**/tests/**/*.test.js"
		]
	}
}
